<script type="text/javascript">
  $( document ).ready(function() {
    updateSidebarHeight();
  });
  $( window ).resize(function() {
    updateSidebarHeight();
  });
  function updateSidebarHeight() {
    if ($(window).width() > 991) {
      if ($("#primary-section").height() > 3900) {
        $("#sidebar").addClass("placeholder-side-banner");
          var pos = $("#primary-section").position();
          var pos2 = $(".sidebar").position();
          var diff = pos2.top - pos.top;
          $(".sidebar").height($("#primary-section").height() - diff);
      } else {
          $("#sidebar").removeClass("placeholder-side-banner");
          $(".sidebar").css({"height":"auto"});
      }
    } else {
        $(".sidebar .sidebar-item").css({"float":"none"});
        $("#sidebar").removeClass("placeholder-side-banner");
        $(".sidebar").css({"height":"auto"});
    };
  }
</script>
